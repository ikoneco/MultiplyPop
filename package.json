{
  "name": "multiply-pop",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "start:clear": "expo start --clear",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "expo lint",
    "typecheck": "tsc --noEmit",
    "emulators": "firebase emulators:start",
    "emulators:export": "firebase emulators:start --export-on-exit=./emulator-data",
    "emulators:import": "firebase emulators:start --import=./emulator-data",
    "build:web": "expo export --platform web",
    "deploy:hosting": "npm run build:web && firebase deploy --only hosting",
    "deploy:rules": "firebase deploy --only firestore:rules,storage",
    "eas:build:dev": "eas build --profile development",
    "eas:build:preview": "eas build --profile preview",
    "eas:build:prod": "eas build --profile production"
  },
  "dependencies": {
    "@expo/metro-runtime": "~6.1.2",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@tamagui/config": "^1.138.6",
    "@tamagui/core": "^1.138.6",
    "expo": "~54.0.26",
    "expo-auth-session": "~7.0.9",
    "expo-constants": "~18.0.10",
    "expo-crypto": "~15.0.7",
    "expo-linking": "~8.0.9",
    "expo-router": "~6.0.16",
    "expo-status-bar": "~3.0.8",
    "expo-updates": "~29.0.14",
    "expo-web-browser": "~15.0.9",
    "firebase": "^12.6.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-web": "^0.21.0",
    "tamagui": "^1.138.6",
    "zod": "^4.1.13",
    "zustand": "^5.0.9"
  },
  "devDependencies": {
    "@tamagui/babel-plugin": "^1.138.6",
    "@testing-library/react": "^16.3.0",
    "@types/react": "~19.1.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "babel-preset-expo": "^54.0.7",
    "jsdom": "^27.2.0",
    "typescript": "~5.9.2",
    "vitest": "^4.0.15"
  },
  "private": true
}